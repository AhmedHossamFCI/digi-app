/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@app/env": [
        "../src/environments/environment"
      ],
      "@app/shared": [
        "app/shared"
      ],
      "tslib": [
        "path/to/node_modules/tslib/tslib.d.ts"
      ]
    },
  },
  "files": [
    "../src/main.ts"
  ],
  "include": [
    "../src/**/*.d.ts"
  ]
}